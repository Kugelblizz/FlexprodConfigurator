query findGlobalHashByUserId {
  description: "Select all global hashes with certain userId"
  statement:
      SELECT at.jku.cis.globalHash
         WHERE (userId == _$userId)
}

query findSimpleHash {
  description: "Check if a given simple hash is in blockchain or not"
  statement:
      SELECT at.jku.cis.simpleHash
         WHERE (hash == _$hash)
}